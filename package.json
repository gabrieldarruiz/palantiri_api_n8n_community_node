{
  "name": "n8n-nodes-palantiriapi",
  "version": "0.1.0",
  "description": "n8n community node for palantiriAPI (WhatsApp API)",
  "license": "MIT",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "whatsapp",
    "palantiri"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrieldarruiz/palantiri_api_n8n_community_node.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PalantiriApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PalantiriApi/PalantiriApi.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "n8n-workflow": "^2.8.0",
    "typescript": "~5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18"
  }
}
